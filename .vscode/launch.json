{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: FastAPI Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Sample Doc Generation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\docgen_sample.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Setup New Map",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\setup_new_map.py",
            "console": "integratedTerminal"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against ng-serve",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}\\ngapp"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against FastAPI",
            "url": "http://localhost:8000",
            "webRoot": "${workspaceFolder}\\ngapp"
        },
    ]
}